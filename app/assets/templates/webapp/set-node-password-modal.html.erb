<div class="outer-modal-container ">
  <div class="modal-container modal-ready-to-appear">
    <div class="modal-header">
      <span class="modal-title">{{ 'SET_PSW_FOR' | translate }}{{nameNode}}</span>
      <span class="fa fa-times close-modal" ng-click="dismissModal()"></span>
    </div>
    <div class="modal-body">
      <input class="modal-input" ng-model="nodePassword" type="password" required>
      <div ng-click="setPassword()" class="modal-button">{{ 'SET_PSW' | translate }}</div>
    </div>
  </div>
</div>

<div class="modal-background"></div>
